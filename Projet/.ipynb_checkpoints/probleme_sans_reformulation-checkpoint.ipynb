    "# Recuperation des arcs\n",
    "function getEdge(kep)\n",
    "    edge_list = Array{Array{Int64,1},1}()\n",
    "    for (ind,arc) in enumerate(edges(kep))\n",
    "            push!(edge_list,[src(arc), dst(arc)])\n",
    "    end\n",
    "    return edge_list\n",
    "end"
   ]
